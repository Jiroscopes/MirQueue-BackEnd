{
	"name": "mirqueue",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"start": "node .",
		"dev": "tsc && node .",
		"watch": "nodemon",
		"test": "jest --testTimeout=10000"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts,json",
		"ignore": [
			"src/**/*.spec.ts"
		],
		"exec": "ts-node ./src/index.ts"
	},
	"author": "Steven Popick",
	"dependencies": {
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cuid": "^2.1.8",
		"dotenv": "^8.6.0",
		"express": "^4.17.1",
		"express-session": "^1.17.3",
		"mariadb": "^2.5.5",
		"ws": "^7.4.6"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/express-session": "^1.17.5",
		"@types/node": "^16.11.19",
		"@types/uuid": "^8.3.4",
		"@types/ws": "^8.5.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.5.4"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		]
	}
}
